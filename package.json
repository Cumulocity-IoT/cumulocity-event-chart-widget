    "@angular-devkit/build-ng-packagr": "~0.803.27",
    "@angular/cli": "~8.3.8",
    "@angular/cdk": "8.2.3",
    "@angular/compiler-cli": "~8.2.9",
    "@angular/language-service": "~8.2.9",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
